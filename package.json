    "eslint-plugin-unicorn": "^43.0.0",
    "express": "^4.18.1",
    "express-prom-bundle": "^6.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prom-client": "^14.0.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
